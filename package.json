{
  "main": "dist/worker.js",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "build:worker": "esbuild --bundle --define:process.env.NODE_ENV='\"production\"' --minify --outdir=dist --sourcemap ./worker",
    "deploy": "npm run build && wrangler publish",
    "dev": "remix watch",
    "dev:worker": "esbuild --bundle --define:process.env.NODE_ENV='\"development\"' --outdir=dist --sourcemap ./worker",
    "postinstall": "remix setup cloudflare-workers",
    "start": "miniflare --build-command \"npm run dev:worker\" --watch",
    "test:type-check": "tsc"
  },
  "dependencies": {
    "@remix-run/cloudflare-workers": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@remix-run/dev": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.22",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "esbuild": "^0.14.21",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "miniflare": "^2.3.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
